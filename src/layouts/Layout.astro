	<body>
		<Navbar/>
			<div class="bg-slate-600 h-[2000px]">
			<nav class="bg-white h-[21.2vh]"></nav>
				<img src="/assets/WhitePaw.png" alt="Paw" class="relative ml-[60vw] top-[] animate-[wiggle_1s_ease-in-out_infinite]"/>
			 <div class=" lg:2/6 xl:w-2/4 mt-20 lg:mt-40 lg:ml-16 text-left">
				<h2 class="text-6xl font-semibold text-gray-900 ">Best Friend with Happy Time</h2>
